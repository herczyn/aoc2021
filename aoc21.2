newPos = function(pos, roll) {
	pos+=roll;
	if (pos>10) {
		pos-=10;
	}
	return pos;
}

results = function(pos1, score1, pos2, score2) {
	if (score2 + pos2>=21) {
		return [0, 1];
	} else {
		var opt3 = results(pos2, score2 + pos2, newPos(pos1, 3), score1)
		var opt4 = results(pos2, score2 + pos2, newPos(pos1, 4), score1)
		var opt5 = results(pos2, score2 + pos2, newPos(pos1, 5), score1)
		var opt6 = results(pos2, score2 + pos2, newPos(pos1, 6), score1)
		var opt7 = results(pos2, score2 + pos2, newPos(pos1, 7), score1)
		var opt8 = results(pos2, score2 + pos2, newPos(pos1, 8), score1)
		var opt9 = results(pos2, score2 + pos2, newPos(pos1, 9), score1)
		return [opt3[1] + opt4[1]*3 + opt5[1]*6 + opt6[1]*7 + opt7[1]*6 + opt8[1]*3 + opt9[1], opt3[0] + opt4[0]*3 + opt5[0]*6 + opt6[0]*7 + opt7[0]*6 + opt8[0]*3 + opt9[0]]
	}
}


